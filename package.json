{
  "name": "cozy-konnector-toutatice",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:konnectors/toutatice.git"
  },
  "keywords": [],
  "author": "Cozy Cloud",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-konnector-dev",
    "standalone": "cozy-konnector-standalone",
    "pretest": "npm run clean",
    "jest": "jest",
    "clean": "rm -rf ./data",
    "build": "webpack",
    "token": "NODE_ENV=development node scripts/fetchToutaticeToken.js",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-$npm_package_repository_url}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build}) --space academie_rennes --verbose",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-client": "13.21.0",
    "cozy-konnector-libs": "4.35.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "26.6.3",
    "json-loader": "0.5.7",
    "lodash": "4.17.20",
    "p-limit": "2.3.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "6.2.1",
    "cozy-app-publish": "0.25.1",
    "cozy-jobs-cli": "1.13.7",
    "eslint-config-cozy-app": "2.0.0",
    "git-directory-deploy": "1.5.1",
    "husky": "4.3.0",
    "mockdate": "3.0.2",
    "open": "7.3.0",
    "randomstring": "1.1.5",
    "svgo": "1.3.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
  }
}
